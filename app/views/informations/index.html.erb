
<% @informations.each do |information|%>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Handle</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row">1</th> 
           <% if information.image? %>
      <td><%= image_tag information.image.url %></td>
           <% else %>
      <td><%= image_tag "noimage.png"%></td>
           <% end %>
      </td>
      <td></td>
      <td>@mdo</td>
    </tr>
  </tbody>
</table>
<% end %>
